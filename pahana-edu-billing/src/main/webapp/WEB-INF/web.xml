<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ProductController</servlet-name>
  	<display-name>ProductController</display-name>
  	<description></description>
  	<servlet-class>ProductController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductControllerr</servlet-name>
  	<display-name>ProductControllerr</display-name>
  	<description></description>
  	<servlet-class>pahana.controller.ProductControllerr</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProductController</servlet-name>
  	<url-pattern>/ProductController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductControllerr</servlet-name>
  	<url-pattern>/ProductControllerr</url-pattern>
  </servlet-mapping>
</web-app>
